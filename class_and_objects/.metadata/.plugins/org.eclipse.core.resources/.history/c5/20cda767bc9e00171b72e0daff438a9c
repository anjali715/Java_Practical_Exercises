import java.util.*;


public class factorial {
	
	public static long fact(int num)
    {
		if (num==1) return 1;
		else
			return num * fact(num - 1);
        
    }
	public static void main(String[] args)
	{
		Scanner scan = new Scanner(System.in); 
		System.out.println("Enter number: ");
		int n = scan.nextInt();
		System.out.println(fact(n));
	}

}
